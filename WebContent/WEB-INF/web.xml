<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Extreme</display-name>
  	<listener>
		<listener-class>com.cdsxt.listener.UserListener</listener-class>
	</listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>ApplyServlet</servlet-name>
    <servlet-class>com.cdsxt.action.ApplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ApplyServlet</servlet-name>
    <url-pattern>/applyServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>com.cdsxt.action.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/adminServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FangAction</servlet-name>
    <servlet-class>com.cdsxt.action.FangAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FangAction</servlet-name>
    <url-pattern>/fAction</url-pattern>
  </servlet-mapping>

  	<filter>
		<filter-name>encodingFilter</filter-name>
		<filter-class>com.cdsxt.filter.EncodingFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>encodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
   <servlet>
    <servlet-name>ResidentTest</servlet-name>
    <servlet-class>com.cdsxt.action.ResidentTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResidentTest</servlet-name>
    <url-pattern>/residentTest</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>CarServlet</servlet-name>
  	<servlet-class>com.cdsxt.action.CarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CarServlet</servlet-name>
  	<url-pattern>/carServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>employeeServelet</servlet-name>
    <servlet-class>com.cdsxt.action.EmployeeServelet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>employeeServelet</servlet-name>
    <url-pattern>/employeeServelet</url-pattern>
  </servlet-mapping>
</web-app>